;; FILE FOR PERSON'S FUNCTIONS

to person-act 
	if state = 0 [
		if random 100 < 1 [
			let current_position current_vertice
			; Changing his status
			set state 1

			; Selecting a different vertice
			set destination one-of vertices with [self != current_position]
		]
	]
end

to get-in
	set state 2
	hide-turtle
end

to get-out
	set state 0
	set current_vertice destination
	move-to destination
	show-turtle
end
